<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="com.vaibhavmojidra.iconsdemo.controller.App">
	<Shell>
		<App>
			<pages>
				<Page title="Page Title: Icons Demo">
					<content>
						<Panel 
						headerText="Panel Title: Icons Demo"
						class="sapUiResponsiveMargin"
							width="auto">
							<content>
								<Button 
									text="I am Button with Icon"
									icon="sap-icon://information"
									press=".onIconDialogButtonClick"/>
							</content>
						</Panel>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>